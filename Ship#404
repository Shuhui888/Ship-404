public class Scene507 : IScene { //5.7,5.9
  public IScene scene_contents() {
    main.Worm++;
    Console.Clear();
    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("You had a big bite of the little cake covered with cream; sweetness filled your taste buds. ");  

    Console.WriteLine("The cake was better than you thought it would be, your body felt light the moment you swallowed it.  "); 

    if (main.Alone == 1)
    {
    Console.WriteLine("Your two new roommates didn’t grab a piece, as the atmosphere between them two is tense. ");  

    Console.WriteLine("Molly seemed very uncomfortable with the fact that Vv had caused her previous roommate’s death. ");  

    Console.WriteLine("And Vv is not happy with Molly’s skeptical attitudes. ");

    Console.WriteLine("You didn’t want this mood to last for the entire night, so you tried to comfort them two.  ");

    Console.WriteLine("Weirdly, they both seemed very obedient to you. ");
    Console.WriteLine(". . .");
    }
    else
    {
      Console.WriteLine(". . .");
    }

    Console.ForegroundColor = ConsoleColor.DarkGray; 

    Console.WriteLine("It is currently 6:55pm. ");  

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("People, in groups of three or one, walked together towards the cabins. "); 
    if (main.Alone == 1)
    {
    Console.WriteLine("The three of you decided to stay at Molly’s cabin, as it was number 25, which was somewhere in the middle of the group of cabins. Being middle always seems the safest. "); 

    Console.WriteLine("But as the door clicks open, the dead body of Molly’s previous roommate fell from the ceiling with a ‘Thump’. "); 

    Console.WriteLine("The girl’s face is purple with her eyes popping open. "); 

    Console.WriteLine("Molly screamed, and many other screams came from the other cabins. "); 
    Console.ForegroundColor = ConsoleColor.DarkGreen; 

    Console.Write("‘No way am I going to stay here for the night with that thing!’ "); 

    Console.ForegroundColor = ConsoleColor.White; 
    Console.WriteLine("Vv covered her nose with disgust. "); 
    Console.WriteLine("After the fear had passed, Molly seemed upset to see the body. She glared at Vv with a disapproving look. "); 
    Console.WriteLine("But she agreed that it isn’t a good idea to sleep where a person was killed. "); 
    Console.WriteLine("The three of you then walked towards your cabin, cabin 13. "); 

    }
    else
    {
      Console.WriteLine("You also walked towards your cabin 13.  ");
      Console.WriteLine("But on the way to your cabin, you saw a few passengers screaming when they opened the door. You looked and saw dead bodies falling from the ceiling.  ");
      Console.ForegroundColor = ConsoleColor.Red; 
      Console.WriteLine("[The monster here sure finds pleasure from scaring people]  ");
      Console.ForegroundColor = ConsoleColor.White; 
      Console.WriteLine("Although you were brainwashing yourself to not be scared by the ‘jokes’ of the monster, you were still tensed up as you began scanning your wristband.   ");
      Console.WriteLine("You felt a bit remorse for staying alone. But you then convinced yourself that your action was correct.  ");
      Console.ForegroundColor = ConsoleColor.Red; 
      Console.WriteLine("[It would be scarier if the person I’m lying next to turn into a zombie]  ");
    }
    Console.ForegroundColor= ConsoleColor.DarkYellow; 

    Console.WriteLine("Press any key to continue. . . ");  
    Console.ReadLine();
    if (main.Alone == 1)
    {
      return new Scene509();
    }
    else
    {
      return new Scene609();
    }
  }
}
