public class Scene301 : IScene {
  public IScene scene_contents() {
    Console.Clear();
    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("You started to eat.   "); 

    Console.WriteLine("The food tasted interesting, it's weirdly greasy with a tint of sweetness, but the smell made the dish much appetizing. "); 

    Console.WriteLine("You ignored the weird taste and convinced yourself that the seafood in this area of ocean tastes different than the ones you've eaten before. "); 

    Console.WriteLine("You finished your meal. "); 

    Console.ForegroundColor = ConsoleColor.DarkGray; 

    Console.WriteLine("It is currently 11:16 am.");  

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("Some people started to leave the cafeteria, while others stayed. You and Kea stood up to leave.  "); 

    Console.WriteLine("Suddenly a scream shrilled up your spines.  "); 

    Console.WriteLine("You turned around and saw Kea being ripped apart by the waitress that doesn't look human anymore.  "); 

    Console.WriteLine("A bright shade of red filled your sight. Pain screeched through your head. "); 

    Console.WriteLine(". . ."); 
    Console.ForegroundColor = ConsoleColor.DarkYellow; 

    Console.WriteLine("Press any key to continue..."); 
    Console.ReadLine();
    Console.Clear();
    Console.ForegroundColor = ConsoleColor.DarkGray; 

    Console.WriteLine("You are slayed by "); 

    Console.ForegroundColor = ConsoleColor.Magenta; 

    Console.WriteLine(" [Waitress] "); 

    Console.ForegroundColor = ConsoleColor.DarkGray; 

    Console.WriteLine("You have won title "); 

    Console.ForegroundColor = ConsoleColor.Magenta; 

    Console.WriteLine(" [Meat Lover] "); 

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("* * *"); 
     Console.WriteLine("Op: Retry (1) / Exit (2)"); 
    return main.options(new Scene1(), new Scene0(), null);
  }
}
public class Scene302 : IScene {
  public IScene scene_contents() {
    Console.Clear();
    Console.ForegroundColor = ConsoleColor.Green; 

    Console.WriteLine("'Sorry, I'm in a hurry,' "); 

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("you said warily and stepped away from the crewmate. "); 

    Console.WriteLine("With no alarm, he suddenly sprang up from the ground, his arm flying towards your neck.  "); 

    Console.WriteLine("Your pupils narrowed, out of instinct, you ducked. His body flew over you and landed flat on the ground. "); 

    Console.WriteLine("You saw in horror that his body drained away onto the floor, leaving his uniform lying where he landed.  "); 

    Console.WriteLine("A shadow of dark red soaked the blue uniform into black. "); 

    Console.WriteLine("You went silent as the fright shilled through your veins.  "); 

    Console.WriteLine("You ran back to the cabin. "); 

    Console.WriteLine(". . . "); 

    if (main.Kea == 1)
    {
      Console.WriteLine("Kea is still sound asleep, you tried to wake her up, but she didn't move, still heavily asleep."); 
    }
    else
    {
      Console.WriteLine("Kea hasn't come back.  "); 
    }

    Console.WriteLine("The horrific image is still playing again and again in your head.  "); 

    Console.WriteLine("You had to do something to calm yourself down. "); 

    Console.WriteLine("You decide to  "); 

    Console.ForegroundColor = ConsoleColor.DarkYellow; 

    Console.WriteLine("Op: Stay awake (1) / try to go to sleep (2) ");

  return main.options(new Scene401(), new Scene306(), null);
  }
}
public class Scene303 : IScene {
  public IScene scene_contents() {
    main.Crew++; 
    Console.Clear();
    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("You gave your wristband to him nervously, afraid that he might not give it back. "); 

    Console.WriteLine("He took it and grinned. You saw him opening the back of the wrist band, glanced inside, and closing it back on. "); 

    Console.WriteLine("You relaxed when he gave the wristband back.  "); 

    Console.ForegroundColor = ConsoleColor.DarkGreen; 

    Console.Write("'Thank you miss. It's not the one.'"); 

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine( " His grin was so wide it was unavoidable. "); 

    Console.WriteLine("You nodded to him uncertainly. He nodded back and jogged away as if he was in a hurry. "); 

    Console.ForegroundColor = ConsoleColor.Red; 

    Console.WriteLine("[This is mad. Everything today is so un-normal. The announcement, the paper, my roommate, and this random guy. This must be a dream.] ");  

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine( "You walked around the ship. There were few people around. You passed the cafeteria and heard some screeching noises and a really nice smell.  "); 

    Console.WriteLine( "But you felt a bit seasick, so you decided to go back to the cabin for some rest.  "); 

    Console.ForegroundColor = ConsoleColor.DarkGray; 

    Console.WriteLine("It is currently 11:57 am.");  

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine(". . ."); 

    Console.WriteLine("You opened the cabin door and suddenly felt very tired."); 

    Console.WriteLine("You decide to  "); 

    Console.ForegroundColor = ConsoleColor.DarkYellow; 

