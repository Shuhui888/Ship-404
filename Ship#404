public class Scene404 : IScene {
  public IScene scene_contents() {
    Console.Clear();
    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("A loud bell rang, and clinking noise shook the whole deck.  "); 

    Console.WriteLine("A man wearing a blue vest and poofy sleeves rose up from the middle of the stage.  "); 

    Console.WriteLine("He had painted his face into some kind of joker face -- white skin, two red circles on his cheek, thick eyeliners and a grin that lid up all the way to his ears. His hair was greasy white, it stuck to his skull as if someone poured a bucket of water over him. "); 

    Console.ForegroundColor = ConsoleColor.DarkGreen; 

    Console.Write("'I always love this grand entrance, hello – my dear playmates~, welcome to the show tonight!' "); 

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("His grin wasn't drawn, the cut on his cheek opened and closed.  "); 

    Console.WriteLine("The audience replied with silence, only a few tiny gasps and whimpers were heard. "); 

    Console.ForegroundColor = ConsoleColor.DarkGreen; 

    Console.Write("'Awwwww no applause? This is disappointing.'"); 

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("His head drooped down for a moment, then it rose up again."); 

    Console.ForegroundColor = ConsoleColor.DarkGreen; 

    Console.WriteLine("'Well, it's always like this every year, I guess I can't get my hopes up... But I am a very caring man and I'm known for my kindness and forgiveness, hehe~' "); 

    Console.Write("'I will forgive you for your disrespect, and still give you the chance to demonstrate with my magic show~ Ummm lets see... Who volunteers?' "); 

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("The joker expressed his words with exaggerated body motion. "); 

    Console.WriteLine("The audience remained silent. "); 

    Console.ForegroundColor = ConsoleColor.DarkGreen; 

    Console.Write("'An audience that doesn't cooperate with the performer isn't a good audience... But luckily for you, we got people who registered to participate beforehand!' "); 

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine(" His eyes opened wide with excitement.  "); 

    Console.ForegroundColor = ConsoleColor.DarkGreen; 

    Console.WriteLine("'Let's welcome... Ashley Watson!'"); 

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("Some rusty music sang, and a flash of dark red light from above shined on a girl's head.  "); 

    Console.WriteLine("She looked around 20, with bronze hair and a pretty face. Her face turned pale as the light stopped above her, and her whole body trembled on the seat. "); 

    Console.WriteLine("Shaking, she stood up and ran towards the stairs. "); 

    Console.WriteLine("The joker melted on the stage and appeared right in the way she was running towards.  "); 

    Console.WriteLine("He grabbed her arms and pulled her towards the stage. "); 

    Console.WriteLine("She screamed and cried as he stuffed her into a black cabinet. There is a hole in the top of the cabinet. Her head was outside the cabinet while her body was inside."); 

    Console.ForegroundColor = ConsoleColor.DarkYellow; 

    Console.WriteLine("Press any key to continue... "); 

    Console.ReadLine();
    Console.Clear();

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("The joker placed a black box over her head, then he pulled out a long, medal blade from the ground. "); 

    Console.WriteLine("He started to spin the box, and aimed the blade at the spinning box where the girl's head is in. "); 

    Console.WriteLine("Everyone in the audience held their breath, and some closed their eyes. "); 

    Console.WriteLine("Without hesitation the blade was stabbed into the box, and the other side came out from the other side."); 

    Console.WriteLine("The spinning box was forced to a stop. "); 

    Console.WriteLine("Silent. "); 

    Console.WriteLine("The joker, with his lips widened to his cheekbones, pulled the blade back out and threw it to the ground, then he gracefully held the black box off the girl's head, and opened the cabinet. "); 

    Console.WriteLine("The girl, trembling, was standing there, unwounded. "); 

    Console.WriteLine("The audience is still silent. You were frozen by the tense atmosphere. "); 

    Console.Write("The joker looked around, looking proud. "); 

    Console.ForegroundColor = ConsoleColor.DarkGreen; 

    Console.WriteLine("'Well, where's the applause?' "); 

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("The only noise heard was the girl's whimpers. "); 

    Console.WriteLine("He suddenly seemed mad by the audience's reaction. He grabbed the blade from the ground and sliced through the girl's head. "); 

    Console.WriteLine("Red and white splashed over the stage. "); 

    Console.WriteLine("Screams rang the deck, and people started to run towards the exit. "); 

    Console.WriteLine("You decide to "); 

    Console.ForegroundColor = ConsoleColor.DarkYellow; 

    Console.WriteLine("Op: Run away (1) / Stay (2) "); 

    return main.options(new Scene407(), new Scene408(), null);
  }
}
public class Scene405 : IScene {
  public IScene scene_contents() {
    Console.Clear();
    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("A loud bell rang, and clinking noise shook the whole deck.  "); 

    Console.WriteLine("A man wearing a blue vest and poofy sleeves rose up from the middle of the stage.  "); 

    Console.WriteLine("He had painted his face into some kind of joker face -- white skin, two red circles on his cheek, thick eyeliners and a grin that lid up all the way to his ears. His hair was greasy white, it stuck to his skull as if someone poured a bucket of water over him. "); 

    Console.ForegroundColor = ConsoleColor.DarkGreen; 

    Console.Write("'I always love this grand entrance, hello – my dear playmates~, welcome to the show tonight!' "); 

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("His grin wasn't drawn, the cut on his cheek opened and closed.  "); 

    Console.WriteLine("The audience replied with silence, only a few tiny gasps and whimpers were heard. "); 

    Console.ForegroundColor = ConsoleColor.DarkGreen; 

    Console.Write("'Awwwww no applause? This is disappointing.'"); 

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("His head drooped down for a moment, then it rose up again."); 

    Console.ForegroundColor = ConsoleColor.DarkGreen; 

    Console.WriteLine("'Well, it's always like this every year, I guess I can't get my hopes up... But I am a very caring man and I'm known for my kindness and forgiveness, hehe~' "); 

    Console.Write("'I will forgive you for your disrespect, and still give you the chance to demonstrate with my magic show~ Ummm lets see... Who volunteers?' "); 

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("The joker expressed his words with exaggerated body motion. "); 

    Console.WriteLine("The audience remained silent. "); 

    Console.ForegroundColor = ConsoleColor.DarkGreen; 

    Console.Write("'An audience that doesn't cooperate with the performer isn't a good audience... But luckily for you, we got people who registered to participate beforehand!' "); 

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine(" His eyes opened wide with excitement.  "); 

    Console.ForegroundColor = ConsoleColor.DarkYellow; 

    Console.WriteLine("Press any key to continue. . .");
    Console.ReadLine();
    Console.Clear();
    Console.ForegroundColor = ConsoleColor.White; 
    Console.WriteLine("Some rusty music sang, and a flash of dark red light from above shined on your head.   "); 
    Console.WriteLine("You were terrified, your heart nearly stopped beating. "); 
    Console.WriteLine("You felt your blood run cold and your limbs stiffened. "); 
    Console.WriteLine("You stood up and ran towards the exit with all your strength, regretting sitting in the middle but not the very back row. "); 
    Console.WriteLine("You bumped into someone’s chest; the Joker somehow was standing right in front of you with his cracked smile. "); 
    Console.WriteLine("You screamed as he held you by your arms and pulled you towards the stage. "); 
    Console.WriteLine("He stuffed you into a black cabinet. There is a hole in the top of the cabinet. "); 
    Console.WriteLine("Your head was out through the hole facing the audience while your body was in the cabinet. "); 

    Console.WriteLine("The joker placed a black box over your head, then he pulled out a long, medal blade from the ground. "); 

    Console.WriteLine("He started to spin the box fast and aimed the blade at the spinning box at your head. "); 

    Console.WriteLine("You were all dizzy and scared, you felt like you were going to vomit. "); 

    Console.WriteLine("It was some exhausting seconds of silence. "); 

    Console.WriteLine("Suddenly, you heard a blade stabbing through something, but you felt no pain, just a huge force that stopped the cabinet. "); 

    Console.WriteLine("Silent. "); 

    Console.WriteLine("Then you saw light again as the joker gracefully removed the black box from your head. "); 

    Console.WriteLine("You can feel the sweat going down your cheeks "); 

    Console.WriteLine("The audience is still silent.  "); 

    Console.Write("The joker looked around, looking proud. "); 

    Console.ForegroundColor = ConsoleColor.DarkGreen; 

    Console.WriteLine("'Well, where's the applause?' "); 

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("The only noise heard was your whimpers. "); 

    Console.WriteLine("He suddenly seemed mad by the audience's reaction."); 

    Console.WriteLine("He bent down his body and re-grabbed the blade. "); 

    Console.WriteLine("The blade shimmered in front of your eyes, and the last thing you saw was the joker’s feet. "); 

    Console.WriteLine("There was mold on the shoes. "); 
    Console.ForegroundColor = ConsoleColor.DarkYellow; 

    Console.WriteLine("Press any key to continue. . .");
    Console.ReadLine();
    Console.Clear();
    Console.ForegroundColor = ConsoleColor.DarkGray; 

    Console.WriteLine("You are slayed by "); 

    Console.ForegroundColor = ConsoleColor.Magenta; 

    Console.WriteLine(" [Joker] "); 

    Console.ForegroundColor = ConsoleColor.DarkGray; 

    Console.WriteLine("You have won title "); 

    Console.ForegroundColor = ConsoleColor.Magenta; 

    Console.WriteLine(" [Headless Nick] "); 

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("* * *"); 
    Console.ForegroundColor = ConsoleColor.DarkYellow; 
       Console.WriteLine("Op: Retry (1) / Exit (2)");
    return main.options(new Scene1(), new Scene0(), null);
  }
}
