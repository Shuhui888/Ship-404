    Console.WriteLine("You shook your head and stared at your shoes in confusion.  ");
    Console.ForegroundColor = ConsoleColor.Red;
    Console.WriteLine("[This cannot be a dream, can it?] ");
    Console.ForegroundColor = ConsoleColor.White;
