public class Scene308 : IScene {
  public IScene scene_contents() {
    Console.Clear();
    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("You decided that the hunger isn't much, and you felt more comfortable staying in the cabin. "); 

    if (main.Kea == 1)
    {
      Console.ForegroundColor = ConsoleColor.DarkGreen; 

    Console.Write("'You sure you're not coming?' "); 

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("Kea asked. "); 

    Console.ForegroundColor = ConsoleColor.Green; 

    Console.Write("'I'm not really hungry yet, I will go for food later in the day.' "); 

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("Kea left the cabin. "); 

    Console.WriteLine("But after a long while, Kea still hasn't come back. And hunger and boredness started to come over you. "); 
    }
    else
    {
      Console.WriteLine(" "); 

      Console.WriteLine(". . ."); 

      Console.WriteLine("But after a long while, hunger and boredness started to come over you. "); 
    }

    Console.WriteLine("You decide to leave the cabin. "); 

    Console.ForegroundColor = ConsoleColor.DarkGray; 

    Console.WriteLine("It is currently 01:49 pm.");  

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine(". . . "); 

    Console.WriteLine("You soon found out that the cafeteria was closed with all the lights out.   "); 

    Console.WriteLine("You were disappointed and frightened by the fact that you had not seen a living soul on the way here. "); 

    Console.WriteLine("Then you remembered that there was a show provided on the ship at 2 pm on the deck."); 

    Console.ForegroundColor = ConsoleColor.Red; 

    Console.WriteLine("[The people must be at the show, that's why I haven't seen anyone out...] ");  

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("You walked upstairs to the deck. "); 

    Console.ForegroundColor = ConsoleColor.DarkYellow; 

    Console.WriteLine("Press any key to continue... "); 

    Console.ReadLine();

  return new Scene4031();
  }
