using System;
       public interface IScene {
       public IScene scene_contents();
       }

public class main {
       public static string username = "";
  public static int Crew = 1; 
  public static int Kea = 1; 
  public static int Clown = 1; 
  public static int Worm = 1; 
  public static int Alone =1;

      public static string getUserInput() {
                    string? input;

             do {
               input = Console.ReadLine();
             } while (string.IsNullOrEmpty(input));

             return input;
              }

       public static IScene options(IScene a, IScene b, IScene c) {
                    do {
               string option = getUserInput();

               if (option == "1" && a != null) {
                 return a;
               } else if (option == "2" && b != null) {
                 return b;
               } else if (option == "3" && c != null) {
                 return c;
               } else {
                 Console.WriteLine("Invalid input, please try again");
               }
             } while (true);
         return main.options(new Scene1(), new Scene0(), null);
       }

       public static void Main() {
                    IScene current_scene = new Scene1();

             while (current_scene != null) {
                  current_scene = current_scene.scene_contents();

             }
       }
  }

public class Scene1 : IScene {
       public IScene scene_contents() { 
         Console.Clear();
         Console.ForegroundColor = ConsoleColor.White; 
         Console.Write("SHIP");
         Console.ForegroundColor = ConsoleColor.DarkGray; 
         Console.WriteLine("#404");
         Console.ForegroundColor = ConsoleColor.DarkRed; 
         Console.WriteLine("Press any key to began game. . .");
         Console.ReadLine();
         Console.Clear();
         Console.ForegroundColor = ConsoleColor.White; 
              Console.WriteLine("Hello Player.");  
              Console.WriteLine("Welcome aboard to ship #404.");  
              Console.WriteLine("All passengers please follow the instructions given by the officers and crew...");  
              Console.ForegroundColor = ConsoleColor.Blue; 
              Console.WriteLine("1. We will arrive in 2 days, please survive until then. ");  
              Console.WriteLine("2. Every passenger is not allowed to leave your cabin after ten p.m. Do not open your door until 7 a.m. ");  
              Console.WriteLine("Don't go wandering around alone, you will get lost."); 
              Console.WriteLine("3. If Passenger notices that there's someone watching you outside the window, ignore it, do not shut the window because of fear. ");  
              Console.WriteLine("4. On this ship, all the crewmates and officers wear blue. ");  
              Console.WriteLine("5. The cafeteria is open from 7-8, 12-13, and 17-18. Please do not go to the cafeteria at other times. ");  

              Console.WriteLine("6. Please take good care of your wristband, it is the only key to open your cabin door. ");  

              Console.WriteLine("7. The Wi-Fi aboard is NOT free, you are only able to pay for Wi-Fi once a day, the free Wi-Fi is for the crew. ");  

              Console.WriteLine("8. There is a performance from 2-6 pm, all passengers have to be there, ones that aren't there would suffocate. "); 

              Console.WriteLine("9. Always be aware of the time, don't sleep pass the performance.  "); 

              Console.ForegroundColor = ConsoleColor.White; 

              Console.WriteLine("Failure to comply with these rules will result in death, please be aware of this. Enjoy your journey! ");  

              Console.ForegroundColor = ConsoleColor.DarkYellow; 

              Console.WriteLine("Press any key to continue...");  

              Console.ReadLine();
              Console.Clear();
         Console.ForegroundColor = ConsoleColor.White; 

              Console.WriteLine("...");  

              Console.WriteLine("You are: ");  

              main.username = main.getUserInput();

              Console.ForegroundColor = ConsoleColor.DarkGreen; 
              Console.Clear();

              Console.WriteLine("'Hi,"+ main.username + "! Here's your wristband! I'm Kea, I will be your roommate for the following days! We are at cabin 13! Let's go!' ");  

              Console.ForegroundColor = ConsoleColor.White; 

              Console.WriteLine("You are travelling to Russia to attend college. It's your first time travelling overseas. ");

              Console.WriteLine("You took the wristband and followed her to Cabin 13.  ");  

              Console.ForegroundColor = ConsoleColor.DarkGray; 

              Console.WriteLine("It is currently 10:37. ");  

              Console.ForegroundColor = ConsoleColor.White; 

              Console.WriteLine("You found that there is a piece of paper on your bed, you decide to ");  

              Console.ForegroundColor = ConsoleColor.DarkYellow; 

              Console.WriteLine("Op: Pick it up (1) / Ignore it (2) ");  

              Console.WriteLine("//Insert digit 1 or 2 ");  

              return main.options(new Scene201(), new Scene202(), null);
            }
}
public class Scene0 : IScene {
  public IScene scene_contents() {
    Console.Clear();
    Environment.Exit(0);
  return null;
  }
}
public class Scene201 : IScene {
  public IScene scene_contents() {

    Console.Clear();
    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("You picked up the paper. The note reads: ");  

    Console.WriteLine("Dear Player: ");  

    Console.ForegroundColor = ConsoleColor.Blue; 

    Console.WriteLine("  1. Please remember the following rules, try not to disobey them ");  

    Console.WriteLine("  2. Do not eat products that contains flour ");  

    Console.WriteLine("  3. Do not use free Wi-Fi  ");  

    Console.WriteLine("  4. Be careful of what you say, try your best to not cause problems ");  

    Console.WriteLine("  5. THE OCEAN IS BLACK!!!  ");  

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("You are confused. You looked outside and saw the blue ocean.");  

    Console.ForegroundColor = ConsoleColor.Red; 

    Console.WriteLine(" [This is ridiculous.] ");  

    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("You thought.");  

    Console.WriteLine("You decide to");  

    Console.ForegroundColor= ConsoleColor.DarkYellow; 

    Console.WriteLine("Op: Show paper to Kea (1) / Keep paper to yourself (2) ");  
    return main.options(new Scene203(), new Scene204(), null);

    }
  }
