public class Scene603 : IScene {
  public IScene scene_contents() {
    Console.Clear();
    Console.ForegroundColor = ConsoleColor.White;
    Console.WriteLine("The night came.        "); 
    Console.WriteLine("The three of you huddled up on the same bed, you tried hard to close your eyes. "); 
    Console.WriteLine("The window curtains were open, a faint light of blue drifts in.  "); 
    Console.WriteLine("The reason the curtains weren’t closed was because people figured from the broadcasting rules in the morning that it was better to keep the curtains open.  ");
    Console.WriteLine("But you weren’t able to sleep, the fright today has been too much. Meanwhile, Molly and Vv were already looking asleep. ");
    Console.WriteLine("You forced yourself to close your eyes shut. ");
    Console.ForegroundColor = ConsoleColor.DarkRed; 
    Console.WriteLine("'Knock, knock.’ ");
    Console.ForegroundColor = ConsoleColor.White; 
    Console.WriteLine("Knocking sound came from the cabin door, and instantly you trembled and held your breath, and you felt Molly and Vv also holding their breaths. "); 
    Console.WriteLine("The cabin went silent. ");
    Console.WriteLine("Soon the knocking noises got louder and faster. And you heard the knocking person yelling for help. She sounds like Kea.  ");
    Console.ForegroundColor = ConsoleColor.DarkGreen; 
    Console.Write("‘Is that your roommate?’");
    Console.ForegroundColor = ConsoleColor.White; 
    Console.WriteLine(" Vv asked after seeing the looks on your face.");
