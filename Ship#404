public class Scene6 : IScene {
  public IScene scene_contents() {
    Console.Clear();
    Console.ForegroundColor = ConsoleColor.White; 
    Console.WriteLine("The night came.  "); 
    Console.WriteLine("The three of you huddled up on the same bed, you tried hard to close your eyes. "); 
    Console.WriteLine("The window curtains were open, a faint light of blue drifts in.  "); 
    Console.WriteLine("The reason the curtains weren’t closed was because the people figured from the broadcasting rules in the morning that it was better to keep the curtains open. ");
    Console.WriteLine("But you weren’t able to sleep, the fright today has been too much. Meanwhile, Molly and Vv were already looking asleep. "); 
    Console.WriteLine("Then a gush of pain in your stomach forced you to widen your eyes. "); 
    Console.WriteLine("And another gush, and another.  "); 
    Console.WriteLine("You decided that the pain was not ignorable, but goodness the cabins came with washrooms, and it’s only a few steps away from the bed. ");
    Console.WriteLine("You decide to  ");
    Console.ForegroundColor= ConsoleColor.DarkYellow; 
    Console.WriteLine("Op: Go to the washroom (1) / stay in bed (2) ");
  return main.options(new Scene601(), new Scene602(), null);
  }
}
public class Scene601 : IScene {
  public IScene scene_contents() {
    Console.Clear();
    Console.ForegroundColor = ConsoleColor.White; 
    Console.WriteLine("You pushed yourself up from the bed and walked towards the washroom. "); 
    Console.ForegroundColor = ConsoleColor.DarkGreen; 
    Console.WriteLine("‘Where are you going to?’  "); 
    Console.ForegroundColor = ConsoleColor.White; 
    Console.WriteLine("A voice made you jump a little, you realized that it was Molly speaking. "); 
    Console.ForegroundColor = ConsoleColor.Green; 
    Console.WriteLine("‘Wash... washroom.’ ");
    Console.ForegroundColor = ConsoleColor.White; 
    Console.WriteLine("Your voice was trembling as you stumbled towards the washroom door, you could feel things rolling in your stomach. "); 
    Console.WriteLine("You felt it going up your throat. You kneeled in front of the toilet and started vomiting.  "); 
    Console.WriteLine("But it won’t stop. You looked down in the toilet and fear filled your head. You want to scream and call for help, but no sound came out except for guggling.  "); 
    Console.WriteLine("There are white, lumpy worms that’s all about a few inches long, swimming in the toilet water, and more and more are coming out from your mouth. ");
    Console.WriteLine("You heard footsteps and screams of Molly and Vv, but you weren’t able to respond, as you began to feel these slimy things pushing through your eyes, nose and ears. ");
    Console.WriteLine("With a few popping noises, you suddenly lost four out of your five senses, only pain was felt, and your world was fainting into darkness. ");
    Console.ForegroundColor = ConsoleColor.DarkYellow; 
    Console.WriteLine("Press any key to continue...");
    Console.ReadLine();
    Console.Clear();
    Console.ForegroundColor = ConsoleColor.DarkGray; 
    Console.WriteLine("You are slayed by "); 
    Console.ForegroundColor = ConsoleColor.Magenta; 
    Console.WriteLine(" [Wheatworm]  "); 
    Console.ForegroundColor = ConsoleColor.DarkGray; 
    Console.WriteLine("You have won title "); 
    Console.ForegroundColor = ConsoleColor.Magenta; 
    Console.WriteLine(" [Noodle Monster] "); 
    Console.ForegroundColor = ConsoleColor.White; 
    Console.WriteLine("* * *"); 
    Console.ForegroundColor = ConsoleColor.DarkYellow; 
    Console.WriteLine("Op: Retry (1) / Exit (2)");
    return main.options(new Scene1(), new Scene0(), null);
  }
}
