    Console.ForegroundColor = ConsoleColor.DarkYellow; 
    Console.WriteLine("Op: Retry (1) / Exit (2)");
    return main.options(new Scene1(), new Scene0(), null);
  }
}
public class Scene703 : IScene {
  public IScene scene_contents() {
    Console.Clear();
    Console.ForegroundColor = ConsoleColor.White;
    Console.WriteLine("You forced yourself to stay, but moans came from your mouth."); 
    Console.WriteLine("You can feel things rolling in your stomach. "); 
    Console.WriteLine("A huge desire to vomit drowned you, you turned your face towards the ground. ");
    Console.WriteLine("You felt things coming out of your mouth. ");
    Console.WriteLine("It was white, lumpy worms that’s all about a few inches long, all alive and jumping on the cabin floor, some landing on the bed, and more and more are coming out from your mouth. ");
    Console.WriteLine("You want to scream and call for help, but no sound came out except for guggling. You began to regret being alone.");
    Console.WriteLine("Even more desperately, you felt more of these slimy things pushing through your eyes, nose and ears. ");
    Console.WriteLine("With a few popping noises, you suddenly lost four out of your five senses, only pain was felt, and your world was fainting into darkness. ");
    Console.ForegroundColor = ConsoleColor.DarkYellow; 
    Console.WriteLine("Press any key to continue...");
    Console.ReadLine();
    Console.Clear();
    Console.ForegroundColor = ConsoleColor.DarkGray; 
    Console.WriteLine("You are slayed by "); 
    Console.ForegroundColor = ConsoleColor.Magenta; 
    Console.WriteLine(" [Wheatworm]  "); 
    Console.ForegroundColor = ConsoleColor.DarkGray; 
    Console.WriteLine("You have won title "); 
    Console.ForegroundColor = ConsoleColor.Magenta; 
    Console.WriteLine(" [Noodle Monster] "); 
    Console.ForegroundColor = ConsoleColor.White; 
    Console.WriteLine("* * *"); 
    Console.ForegroundColor = ConsoleColor.DarkYellow; 
    Console.WriteLine("Op: Retry (1) / Exit (2)");
    return main.options(new Scene1(), new Scene0(), null);
  }
