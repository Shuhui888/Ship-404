public class Scene307 : IScene {
  public IScene scene_contents() {
    Console.Clear();
    Console.ForegroundColor = ConsoleColor.White; 

    Console.WriteLine("You arrived at the cafeteria and sat down.  "); 

    Console.WriteLine("As if the other passengers also noticed something creepy about this ship, the cafeteria was surprisingly quiet, only some murmuring was heard. "); 

    if(main.Kea == 1)
    {
      Console.WriteLine("Kea seemed surprised by this scene, but out of fear, or the instinct of human beings, she also became quiet. "); 
    }
    Console.WriteLine("You went to the serving table, there were no servers or staff, just two old ladies holding soup spoons.  "); 

    Console.WriteLine("The food was not fancy here, just the normal soup, the soup didn't look appetizing, with lettuce floating on the top, and the incredible smell before that came from the cafeteria is non-existent anymore. "); 

    Console.WriteLine("There was also a bucket of spaghetti beside the soup. "); 

    Console.WriteLine("One of the ladies gave you a bowl of soup and spaghetti. "); 

    Console.WriteLine("The spaghetti looked very greasy and gross, with the sauce clumped in bits. "); 

    Console.WriteLine("You decide to  "); 

